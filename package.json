  "repository": {
    "type": "git",
    "url": "git+https://github.com/cesarandreu/web-app.git"
  },
  "keywords": [
    "web",
    "application"
  ],
  "author": "Cesar Andreu <cesarandreu@gmail.com> (https://github.com/cesarandreu)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/cesarandreu/web-app/issues"
  },
  "standard": {
    "parser": "babel-eslint"
  },
  "babel": {
    "stage": 0
  },
  "homepage": "https://github.com/cesarandreu/web-app#readme",
  "dependencies": {
    "debug": "^2.2.0",
    "normalize.css": "^3.0.3",
    "react": "^0.14.0",
    "react-dom": "^0.14.0"
  },
  "devDependencies": {
    "autoprefixer": "^6.0.3",
    "babel": "^5.8.23",
    "babel-core": "^5.8.25",
    "babel-eslint": "^4.1.3",
    "babel-loader": "^5.3.2",
    "babel-plugin-react-transform": "^1.1.1",
    "babel-runtime": "^5.8.25",
    "css-loader": "^0.19.0",
    "eslint": "^1.6.0",
    "eslint-config-standard": "^4.4.0",
    "eslint-config-standard-react": "^1.1.0",
    "eslint-plugin-react": "^3.5.1",
    "eslint-plugin-standard": "^1.3.1",
    "expect.js": "^0.3.1",
    "extract-text-webpack-plugin": "^0.8.2",
    "file-loader": "^0.8.4",
    "html-webpack-plugin": "^1.6.2",
    "isparta-instrumenter-loader": "^0.2.1",
    "karma": "^0.13.10",
    "karma-coverage": "^0.5.2",
    "karma-mocha": "^0.2.0",
    "karma-phantomjs-launcher": "^0.2.1",
    "karma-sourcemap-loader": "^0.3.5",
